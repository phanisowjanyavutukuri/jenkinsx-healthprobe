From maven:3.3.9
COPY ./ /Fms-Modules/
RUN chmod -R 777 /Fms-Modules/
WORKDIR /Fms-Modules/FmsCarrierAcl
RUN mvn clean package install

From gcr.io/woven-sensor-214209/fms-dependencies:latest
RUN mkdir FmsModules
WORKDIR FmsModules
RUN ls -lrth
COPY --from=0  /FmsModules/ .
entrypoint ["java", "-jar", "/FmsModules/FmsCarrierAcl/target/FmsCarrierAcl.jar", "--spring.config.additional-location=./properties/"]
